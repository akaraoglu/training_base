{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Run Script with Default Config",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/train&test.py",
      "env": {
        "CONFIG_PATH_TRAIN": "parameters/training_config_default.json",
        "CONFIG_PATH_TEST": "parameters/test_config_default.json",
        "TEST_ONLY": "False"
      }
    }
  ]
}